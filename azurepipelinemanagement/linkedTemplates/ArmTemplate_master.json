{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azurepipelinemanagement"
		},
		"LS_CLOUD_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_CLOUD_AzureBlobStorage'"
		},
		"LS_For_Snowflake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_For_Snowflake'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'",
			"defaultValue": "Integrated Security=False;Data Source=MURALIYARLI;Initial Catalog=EMPLOYEE;User ID=kranthi"
		},
		"LS_FOR_SQLSERVER_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azurekeysvalute.vault.azure.net/"
		},
		"LS_Snowflake_AzureBlobStorage_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'LS_Snowflake_AzureBlobStorage'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azurepipelinemanagement_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_CLOUD_AzureBlobStorage_connectionString": {
						"value": "[parameters('LS_CLOUD_AzureBlobStorage_connectionString')]"
					},
					"LS_For_Snowflake_connectionString": {
						"value": "[parameters('LS_For_Snowflake_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"LS_FOR_SQLSERVER_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_FOR_SQLSERVER_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_Snowflake_AzureBlobStorage_sasUri": {
						"value": "[parameters('LS_Snowflake_AzureBlobStorage_sasUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}